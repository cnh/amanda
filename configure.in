dnl Specify --with-krb4 if kerberos bits are in somewhere other than the
dnl listed KRB4_SPOTS.  We only compile kerberos support in if the right 
dnl files are there.

AC_ARG_WITH(krb4-security,
    [   --with-krb4=DIR   kerberos bits can be found here		],
    KRB4_SPOTS="$withval",
    : ${KRB4_SPOTS="/usr/kerberos /usr/cygnus /usr /opt/kerberos"}
)

AC_MSG_CHECKING(kerberos4 bits)
if test -f common-src/krb4-security.c ; then
    for dir in $KRB4_SPOTS; do
	if test "$KRB4_SECURITY" != "yes" && test -f ${dir}/lib/libkrb.a ; then
            AC_MSG_RESULT(found in $dir)
    	    KRB4_SECURITY=yes
    	    AC_DEFINE(KRB4_SECURITY)
	    KRB4INCLUDES=-I$dir/include
	    KRB4LDFLAGS=-L$dir/lib
	    KRB4LIBS="-lkrb -ldes"
	    KRB4COMMON_C=krb4-security.c
	    KRB4COMMON_O=krb4-security.o
	    KRB4CLIENT_C=sendbackup-krb4.c
	    KRB4CLIENT_O=sendbackup-krb4.o
	fi
    done
    
    if test "$KRB4LDFLAGS" = "" ; then
       AC_MSG_RESULT(no libkrb.a)
    fi
else
    AC_MSG_RESULT(no)
fi

